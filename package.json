{
  "name": "jdaughter",
  "version": "0.3.4",
  "description": "Typesafe JSON decoding for TypeScript",
  "main": "dist/common.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm test && npm run build",
    "build": "tsc -m es2015 &&  babel ./dist/index.js -o ./dist/common.js -s",
    "lint": "tslint --project .",
    "test": "mocha -r ts-node/register src/**/*.test.ts"
  },
  "keywords": [
    "typescript"
  ],
  "files": [
    "dist/*"
  ],
  "repository": "git@github.com:grassator/jdaughter.git",
  "author": "Dmitriy Kubyshkin <dmitriy@kubyshkin.name>",
  "license": "MIT",
  "devDependencies": {
    "@types/benchmark": "^1.0.31",
    "@types/mocha": "^5.2.4",
    "@types/node": "^10.5.2",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "benchmark": "^2.1.4",
    "mocha": "^5.2.0",
    "prettier": "^1.13.7",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.9.2"
  }
}
